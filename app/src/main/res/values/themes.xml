<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Ozhinshe" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorOnPrimary">@color/background</item>
        <item name="colorOnBackground">@color/white</item>
        <item name="colorOnSecondary">@color/view</item>
        <item name="colorOnTertiary">@color/view</item>
        <item name="colorOnPrimaryContainer">@color/input</item>
        <item name="colorOnPrimaryFixed">@color/back</item>
        <item name="colorOnPrimaryFixedVariant">@color/back</item>
        <item name="colorOnPrimarySurface">@color/border</item>
        <item name="colorOnSecondaryContainer">@color/zhinshe</item>
    </style>
    <style name="CustomTextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/authborder</item>
        <item name="boxCornerRadiusBottomEnd">12dp</item>
        <item name="boxCornerRadiusBottomStart">12dp</item>
        <item name="boxCornerRadiusTopEnd">12dp</item>
        <item name="boxCornerRadiusTopStart">12dp</item>
        <item name="passwordToggleEnabled">true</item>
        <item name="passwordToggleDrawable">@drawable/show</item>
    </style>
    <style name="CustomTextInputLayout2" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/authborder</item>
        <item name="boxCornerRadiusBottomEnd">10dp</item>
        <item name="boxCornerRadiusBottomStart">10dp</item>
        <item name="boxCornerRadiusTopEnd">10dp</item>
        <item name="boxCornerRadiusTopStart">10dp</item>
        <item name="endIconDrawable">@drawable/union</item>
        <item name="endIconMode">clear_text</item>
    </style>
    <style name="Theme.Ozhinshe" parent="Base.Theme.Ozhinshe" />
    <style name="Background">
        <item name="android:background">@color/white</item>
    </style>
    <style name="Head">
        <item name="android:textColor">@color/background</item>
    </style>
    <style name="Subhead">
        <item name="android:textColor">@color/unselected</item>
    </style>
    <style name="Skip">
        <item name="android:textColor">@color/background</item>
        <item name="backgroundTint">@color/search_text</item>
    </style>
    <style name="UnderLogin">
        <item name="android:textColor">@color/unselected</item>
    </style>
    <style name="Button">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="Button2">
        <item name="backgroundTint">@color/white</item>
    </style>
    <style name="InputBolim">
        <item name="android:background">@color/view</item>
    </style>
    <style name="Profile">
        <item name="android:background">@color/search_text</item>
    </style>
    <style name="Sanattar">
        <item name="android:textColor">@color/border</item>
        <item name="android:backgroundTint">@color/back</item>
    </style>
</resources>