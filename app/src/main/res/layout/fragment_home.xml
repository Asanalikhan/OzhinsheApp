<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    android:id="@+id/nav_host_fragment_home"
    android:name="androidx.navigation.fragment.NavHostFragment"
    app:navGraph="@navigation/nav_graph"
    tools:context=".presentation.basic.HomeFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        style="@style/Background">

        <TextView
            android:id="@+id/ozinshe"
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:background="@drawable/rounded_border"
            android:padding="10dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="60dp"
            app:drawableStartCompat="@drawable/start_logo"
            app:drawableEndCompat="@drawable/zinshe"
            android:drawablePadding="1dp"/>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="16dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/continue_watching"
            android:textSize="16sp"
            style="@style/Head"
            android:fontFamily="sans-serif-medium"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"/>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginTop="32dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/in_trend"
                android:textSize="16sp"
                style="@style/Head"
                android:fontFamily="sans-serif-medium"
                android:layout_gravity="start"/>
            <TextView
                android:id="@+id/barlygy1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/all"
                android:textSize="14sp"
                android:textColor="@color/login"
                android:fontFamily="sans-serif-medium"
                android:layout_gravity="end"/>
        </FrameLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginTop="32dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/for_you"
                android:textSize="16sp"
                style="@style/Head"
                android:fontFamily="sans-serif-medium"
                android:layout_gravity="start"/>

            <TextView

                android:id="@+id/barlygy2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:fontFamily="sans-serif-medium"
                android:text="@string/all"
                android:textColor="@color/login"
                android:textSize="14sp" />
        </FrameLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/chose_genre"
            android:textSize="16sp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            style="@style/Head"
            android:fontFamily="sans-serif-medium"
            android:layout_gravity="start"/>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginTop="32dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/new_zhoba"
                android:textSize="16sp"
                style="@style/Head"
                android:fontFamily="sans-serif-medium"
                android:layout_gravity="start"/>

            <TextView
                android:id="@+id/barlygy3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:fontFamily="sans-serif-medium"
                android:text="@string/all"
                android:textColor="@color/login"
                android:textSize="14sp" />
        </FrameLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginTop="32dp"
            >
            <TextView
                android:layout_width="163dp"
                android:layout_height="wrap_content"
                android:text="Тв-бағдарлама және реалити-шоу"
                android:textSize="16sp"
                style="@style/Head"
                android:fontFamily="sans-serif-medium"
                android:layout_gravity="start"/>

            <TextView
                android:id="@+id/barlygy4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:fontFamily="sans-serif-medium"
                android:text="@string/all"
                android:textColor="@color/login"
                android:textSize="14sp" />
        </FrameLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginTop="32dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/telehikaya"
                android:textSize="16sp"
                style="@style/Head"
                android:fontFamily="sans-serif-medium"
                android:layout_gravity="start"/>

            <TextView
                android:id="@+id/barlygy5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:fontFamily="sans-serif-medium"
                android:text="@string/all"
                android:textColor="@color/login"
                android:textSize="14sp" />
        </FrameLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/by_age"
            android:textSize="16sp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            style="@style/Head"
            android:fontFamily="sans-serif-medium"
            android:layout_gravity="start"/>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginTop="32dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/derekti"
                android:textSize="16sp"
                style="@style/Head"
                android:fontFamily="sans-serif-medium"
                android:layout_gravity="start"/>

            <TextView
                android:id="@+id/barlygy6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:fontFamily="sans-serif-medium"
                android:text="@string/all"
                android:textColor="@color/login"
                android:textSize="14sp" />
        </FrameLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginTop="32dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/shetel"
                android:textSize="16sp"
                style="@style/Head"
                android:fontFamily="sans-serif-medium"
                android:layout_gravity="start"/>

            <TextView
                android:id="@+id/barlygy7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:fontFamily="sans-serif-medium"
                android:text="@string/all"
                android:textColor="@color/login"
                android:textSize="14sp" />
        </FrameLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rcView10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
        <FrameLayout
            android:id="@+id/container_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginTop="32dp" />
    </LinearLayout>
</androidx.core.widget.NestedScrollView>
